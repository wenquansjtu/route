{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/sse",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/message",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/ai-status",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/ai-agents",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/collaborations",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/task-history",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}