{
  "version": 2,
  "public": false,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**", "web/**", "package.json"],
        "maxDuration": 120
      }
    },
    {
      "src": "web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/sse",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/message",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/ai-status",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/ai-agents",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/init-agents",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/collaborations",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/task-history",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ],
  "regions": ["iad1"],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm start",
  "framework": null
}